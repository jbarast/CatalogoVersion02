<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
	      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	      version="3.0">


  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>login</servlet-name>  	
  	<servlet-class>com.ipartek.jonBarnes.webapp.controladores.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>usuariocrud</servlet-name>  	 	
  	<servlet-class>com.ipartek.jonBarnes.webapp.controladores.UsuarioCRUD</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>usuarioform</servlet-name> 	
  	<servlet-class>com.ipartek.jonBarnes.webapp.controladores.UsuarioFormServlet</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>productoscrud</display-name>
  	<servlet-name>productoscrud</servlet-name>
  	<servlet-class>com.ipartek.jonBarnes.webapp.controladores.ProductosCRUDServlet</servlet-class>
  </servlet>
  <servlet>
  	<description>
  	</description>
  	<display-name>productosform</display-name>
  	<servlet-name>productosform</servlet-name>
  	<servlet-class>com.ipartek.jonBarnes.webapp.controladores.ProductosFormServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>usuariocrud</servlet-name>
  	<url-pattern>/usuariocrud</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>usuarioform</servlet-name>
  	<url-pattern>/usuarioform</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>productoscrud</servlet-name>
  	<url-pattern>/productoscrud</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>productosform</servlet-name>
  	<url-pattern>/productosform</url-pattern>
  </servlet-mapping>
</web-app>
